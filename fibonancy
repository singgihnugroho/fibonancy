import javax.swing.*;
public class JavaApplication1 {
        public static void main (String[] args){
	int t=0, p=7, hasil=0;
        String s = JOptionPane.showInputDialog("masukan banyak deret fibonanci : ");
        int x = Integer.parseInt(s);
	for (int i=7; 1<=x; i++){
            t=p;
            p=hasil;
System.out.print(hasil+" ");
hasil=t+p;

	}
}
}
